# Ignore files generated by the operating system
.DS_Store
Thumbs.db

# Ignore files generated by code editors
.vscode/
.idea/

# Ignore build artifacts and dependencies
node_modules/
dist/
build/

# Ignore environment-specific files
.env
.env.local
.env.*.local

# Ignore log files
*.log

# Ignore compiled binaries
*.exe
*.dll
*.so
*.dylib

# Ignore package lock files
package-lock.json
yarn.lock

# Ignore sensitive or personal information
secrets.txt
credentials.json

# Ignore files generated during testing
coverage/